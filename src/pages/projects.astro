---
import MainLayout from "../layouts/mainLayout.astro";
import Project from "../components/project.astro";

import dataProjects from "../data/projects.json";
---

<MainLayout>
    {
        dataProjects.map((item) => (
            <Project
                title={item.title}
                status={item.status}
                image={item.image}
                description={item.description}
                url={item.url}
            />
        ))
    }
</MainLayout>
